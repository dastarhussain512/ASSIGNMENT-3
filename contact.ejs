        .dastar {
            list-style-type: none;
            margin: 0;
            padding: 0;
            overflow: hidden;
            position: fixed;
            top: 0;
            width: 100%;
        }

        .id{
            text-align: center;
            background-color: black;
        }
        section {
            max-width: 400px;
            margin: 20px auto;
            padding: 20px;
            background-color: #fff;
            border-radius: 8px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        }

        label {
            display: block;
            margin-bottom: 8px;
        }

        input {
            width: 20%;
            padding: 8px;
            margin-bottom: 16px;
            box-sizing: border-box;
            border: 1px solid #ccc;
            border-radius: 4px;
        }

        button {
            background-color: #3498db;
            color: #fff;
            padding: 10px 20px;
            border: none;
            border-radius: 4px;
            cursor: pointer;
        }
        .form{
            position: absolute;
   top:50%;
   left:50%;
   width:250px;
        }

        /* Responsive styles */
        @media (max-width: 600px) {
            section {
                max-width: 100%;
            }
        }
        .social-icons {
            display: flex;
            align-items: center;
        }

        .social-icons a {
            margin-right: 10px;
            color: #007bff; /* Change the icon color as needed */
            font-size: 18px;
        }

        .social-icons a:hover {
            color: #0056b3; /* Change the icon color on hover as needed */
        }

        @media (max-width: 768px) {
            .social-icons {
                margin-top: 10px;
                justify-content: flex-end;
            }

            .social-icons a {
                margin-right: 0;
            }
        }
        .video {
            width: 420;
            height: 345;
            margin-top: 21%;
            border-bottom-left-radius: 10px;
            border-top-right-radius: 10px;
            border-bottom-right-radius: 10px;
            border-top-left-radius: 10px;
        }
